{
  "name": "react-editor-repo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "cd ./packages/react-editor && npm i",
    "postinstall": "npm run lib:build",
    "lib:build": "cd ./packages/react-editor && npm run build",
    "lib:watch": "cd ./packages/react-editor && npm run watch",
    "deploy": "npm run build && gh-pages -d ./build",
    "start": "react-scripts start",
    "build": "npm run lib:build && PUBLIC_URL=. react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-editor": "file:packages/react-editor",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "gh-pages": "^2.2.0",
    "jsdom-innertext": "file:packages/jsdom-innertext",
    "jsdom-selection": "file:packages/jsdom-selection",
    "prettier": "^1.19.1",
    "react-scripts": "^3.4.0",
    "typescript": "^3.8.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
